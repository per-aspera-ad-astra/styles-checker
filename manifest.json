{
	"name": "Styles Checker",
	"description": "Check CSS on a page",
	"version": "1.0",
	"manifest_version": 3,
	"action": {
		"default_icon": "images/icon16.png"
	},
	"icons": {
		"16": "images/icon16.png",
		"48": "images/icon48.png",
		"128":"images/icon128.png"
	},
	"permissions": ["activeTab", "scripting"],
	"background": {
		"service_worker": "background.js"
	}
}
