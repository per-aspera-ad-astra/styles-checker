{
	"name": "Styles Checker",
	"description": "Check CSS of any element on a page",
	"version": "1.0.0",
	"manifest_version": 3,
	"permissions": ["activeTab", "scripting"],
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_icon": {
			"16": "/images/icon16.png",
			"32": "/images/icon32.png",
			"48": "/images/icon48.png",
			"128": "/images/icon128.png"
		}
	},
	"icons": {
		"16": "/images/icon16.png",
		"32": "/images/icon32.png",
		"48": "/images/icon48.png",
		"128": "/images/icon128.png"
	}
}
